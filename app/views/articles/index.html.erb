<h1>Articles</h1>
<% @articles.each do |article| %>
    <h3><%= article.title %></h3>
    <p><%= article.text %></p>
    <p><%= link_to 'Modifica articolo', edit_article_path(article) %></p>
    <br>
<% end %>

<%= link_to 'Nuovo articolo', new_article_path %>
<%= link_to 'Nuovo post', controller: 'posts' %>